<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>HR_Help_Desk</display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
    
  </welcome-file-list>
  
  <servlet>
  <servlet-name>viewusers</servlet-name>
  <servlet-class>hr_head.ViewUsers</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>viewusers</servlet-name>
  <url-pattern>/users</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>editusers</servlet-name>
  <servlet-class>hr_head.EditUsers</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>editusers</servlet-name>
  <url-pattern>/edituser</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>updateusers</servlet-name>
  <servlet-class>hr_head.UpdateUsers</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>updateusers</servlet-name>
  <url-pattern>/updateuser</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>deleteusers</servlet-name>
  <servlet-class>hr_head.DeleteUsers</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>deleteusers</servlet-name>
  <url-pattern>/deleteuser</url-pattern>
  </servlet-mapping>
  
  
  
  
  <servlet>
  <servlet-name>viewticket</servlet-name>
  <servlet-class>hr_head.ViewTicketCategory</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>viewticket</servlet-name>
  <url-pattern>/ticket_category</url-pattern>
  </servlet-mapping>
</web-app>